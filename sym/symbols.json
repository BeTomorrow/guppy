{
    "norm":
    {"output":{
        "latex":"||{$1}||",
        "text":"norm({$1})"},
     "attrs":{
	 "type":"norm",
	 "group":"functions"},
     "args":[
	 {"delete":"1"}]
    },
    
    "text":
    {"output":{
        "latex":"\\text{{$1}}",
        "text":"text({$1})"},
     "attrs":{
	 "type":"text",
	 "group":"editor"},
     "args":[
	 {"mode":"text"}]
    },
    
    "sym_name":
    {"output":{
        "latex":"\\backslash\\texttt{{$1}}",
        "text":"SYMBOL({$1})"},
     "attrs":{
	 "type":"symbol",
	 "group":"editor"},
     "attrs":[
	 {"mode":"symbol"}]
    },
    
    "abs":
    {"output":{
        "latex":"\\left|{$1}\\right|",
	"text":"abs({$1})"},
     "attrs":{
	 "type":"absolute_value",
	 "group":"functions"},
     "args":[
	 {"delete":"1"}]
    },
    
    "sqrt":
    {"output":{
        "latex":"\\sqrt{{$1}}",
        "text":"sqrt({$1})"},
     "attrs":{
	 "type":"square_root",
	 "group":"functions"},
     "args":[
	 {"delete":"1"}]
    },
    
    "paren":
    {"output":{
        "latex":"\\left({$1}\\right)",
	"text":"({$1})"},
     "attrs":{
	 "type":"bracket",
	 "group":"functions"},
     "args":[
	 {"delete":"1","is_bracket":"yes"}]
    },
    
    "floor":
    {"output":{
        "latex":"\\lfloor{$1}\\rfloor",
	"text":"floor({$1})"},
     "attrs":{
	 "type":"floor",
	 "group":"functions"},
     "args":[
	 {"delete":"1"}]
    },
    
    "exp":
    {"output":{
        "latex":"{{$1}}^{{$2}}",
	"text":"({$1})^({$2})"},
     "current":{
	 "index":1,
	 "type":"token"},
     "attrs":{
	 "type":"exponential",
	 "group":"functions"},
     "args":[
	 {"up":"2","bracket":"yes","delete":"1","name":"base"},
	 {"down":"1","delete":"1","name":"exponent","small":"yes"}
     ]
    },
    
    "sub":
    {"output":{
        "latex":"{{$1}}_{{$2}}",
	"text":"{$1}{$2}"},
     "current":{
	 "index":1,
	 "type":"token"},
     "attrs":{
	 "type":"subscript",
	 "group":"functions"},
     "args":[
	 {"down":"2","bracket":"yes","delete":"1","name":"base"},
	 {"up":"1","delete":"1","name":"subscript","small":"yes"}
     ]
    },
    
    "frac":
    {"output":{
        "latex":"\\dfrac{{$1}}{{$2}}",
        "small_latex":"\\frac{{$1}}{{$2}}",
	"text":"({$1})/({$2})"},
     "ast":{
	 "value":"/"},
     "attrs":{
	 "type":"fraction",
	 "group":"functions"},
     "args":[
	 {"up":"1","down":"2","name":"numerator"},
	 {"up":"1","down":"2","name":"denominator"}
     ]
    },
    
    "slash":
    {"output":{"latex":"\\dfrac{{$1}}{{$2}}","small_latex":"\\frac{{$1}}{{$2}}","text":"({$1})/({$2})"},
     "current":{
	 "index":1,
	 "type":"token"},
     "ast":{
	 "value":"/"},
     "attrs":{
	 "type":"fraction",
	 "group":"functions"},
     "args":[
	 {"up":"1","down":"2","name":"numerator"},
	 {"up":"1","down":"2","name":"denominator"}
     ]
    },
        
    "int":
    {"output":{
        "latex":"\\displaystyle\\int{{$1}}d{$2}",
        "small_latex":"\\int{{$1}}d{$2}",
	"text":"integrate({$1},{$2})"},
     "attrs":{
	 "type":"indefinite_integral",
	 "group":"calculus"},
     "args":[
	 {"delete":"1","name":"integrand"},
	 {"delete":"1","bracket":"yes","name":"variable"}
     ]
    },
    
    "defi":
    {"output":{
        "latex":"\\displaystyle\\int_{{$1}}^{{$2}}{$3}d{$4}",
        "small_latex":"\\int_{{$1}}^{{$2}}{$3}d{$4}",
	"text":"integrate({$3},{$4},{$1},{$2})"},
     "attrs":{
	 "type":"definite_integral",
	 "group":"calculus"},
     "args":[
	 {"down":"1","up":"2","small":"yes","name":"lower_limit"},
	 {"down":"1","up":"2","small":"yes","name":"upper_limit"},
	 {"down":"1","up":"2","delete":"3","name":"integrand"},
	 {"down":"1","up":"2","bracket":"yes","delete":"4","name":"variable"}
     ]
    },
    
    "deriv":
    {"output":{
        "latex":"\\displaystyle\\frac{d}{d{$1}}{$2}",
        "small_latex":"\\frac{d}{d{$1}}{$2}",
	"text":"diff({$2},{$1})"},
     "attrs":{
	 "type":"derivative",
	 "group":"calculus"},
     "args":[
	 {"down":"1","up":"2","bracket":"yes","name":"function"},
	 {"down":"1","up":"2","bracket":"yes","name":"variable"}
     ]
    },
        
    "sum":
    {"output":{
        "latex":"\\displaystyle\\sum_{{$1}}^{{$2}}{$3}",
        "small_latex":"\\sum_{{$1}}^{{$2}}{$3}",
	"text":"sum({$3},{$1},{$2})"},
     "attrs":{
	 "type":"summation",
	 "group":"functions"},
     "args":[
	 {"down":"1","up":"2","small":"yes","name":"lower_limit"},
	 {"down":"1","up":"2","small":"yes","name":"upper_limit"},
	 {"down":"1","up":"2","delete":"3","bracket":"yes","name":"summand"}
     ]
    },

    "root":
    {"output":{
        "latex":"\\sqrt[{$1}]{{$2}}",
	"text":"nroot({$1},{$2})"},
     "attrs":{
	 "type":"root",
	 "group":"functions"},
     "args":[
	 {"down":"2","up":"1","small":"yes","delete":"1","name":"index"},
	 {"down":"2","up":"1","delete":"1","name":"radicand"}
     ]
    },

    "vec":
    {"output":{
        "latex":"\\left\\langle {$1{,}} \\right\\rangle",
	"text":"vec({$1{,}})"},
     "attrs":{
	 "group":"array",
	 "type":"vector"}
    },

    "mat":
    {"output":{
        "latex":"\\left(\\begin{matrix} {$1{ & }{\\\\}} \\end{matrix}\\right)",
	"text":"matrix({$1{,}{;}})"},
     "attrs":{
	 "group":"array",
	 "type":"matrix"}
    },
    
    "*":
    {"output":{
        "latex":"\\cdot",
	"text":"*"},
     "attrs":{
	 "char":"yes",
	 "group":"operations",
	 "type":"*"},
     "ast":{
	 "type":"operator"}
    },

    "infinity":{"output":{"latex":"\\infty",
			  "text":"$infinity"},
		"attrs":{"group":"qwerty",
			 "char":"yes",
			 "type":"infinity"}
	       },
    
    "_templates":
    [
	{"template":{"output":{"latex":"\\{$name}\\left({$1}\\right)",
			       "text":" {$name}({$1})"},
		     "attrs":{
			 "type":"{$name}",
			 "group":"functions"},
		     "args":[
			 {"delete":"1"}
		     ]},
	 "values":["sin","cos","tan","sec","csc","cot","log","ln"]},
	
	{"template":{"output":{"latex":"\\{$name}",
			       "text":"${$name}"},
		     "attrs":{
			 "group":"greek",
			 "char":"yes",
			 "type":"{$name}"}},
	 "values":["alpha","beta","gamma","delta","epsilon","zeta","eta","theta","iota","kappa","lambda","mu","nu","xi","omicron","pi","rho","sigma","tau","upsilon","phi","chi","psi","omega","Gamma","Delta","Theta","Lambda","Xi","Pi","Sigma","Phi","Psi","Omega"]},

	{"template":{"output":{"latex":"{$latex}",
			       "text":"{$text}"},
		     "attrs":{
			 "group":"operations",
			 "char":"yes",
			 "type":"{$name}"}},
	 
	 "values":{"leq":{"latex":"\\leq","text":" <= "},
		   "less":{"latex":"<","text":" < "},
		   "geq":{"latex":"\\geq","text":" >= "},
		   "greater":{"latex":">","text":" > "}}}
	
    ]
}
